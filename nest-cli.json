{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "postgre": {
      "type": "library",
      "root": "libs/postgre",
      "entryFile": "index",
      "sourceRoot": "libs/postgre/src",
      "compilerOptions": {
        "tsConfigPath": "libs/postgre/tsconfig.lib.json"
      }
    },
    "orm-repositories": {
      "type": "library",
      "root": "libs/orm-repositories",
      "entryFile": "index",
      "sourceRoot": "libs/orm-repositories/src",
      "compilerOptions": {
        "tsConfigPath": "libs/orm-repositories/tsconfig.lib.json"
      }
    },
    "typeorm": {
      "type": "library",
      "root": "libs/typeorm",
      "entryFile": "index",
      "sourceRoot": "libs/typeorm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/typeorm/tsconfig.lib.json"
      }
    },
    "sequelize": {
      "type": "library",
      "root": "libs/sequelize",
      "entryFile": "index",
      "sourceRoot": "libs/sequelize/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sequelize/tsconfig.lib.json"
      }
    }
  }
}